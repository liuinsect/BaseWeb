        <div id="n_middle" style="background-image: url('/misc/images/new_center_middle.gif'); ;width:100%; background-repeat:repeat">
	        <div id="detail" >
			        <div  id="maintab" width="100%" >  
			            <div>
			            	<img src="/misc/images/table_header.gif" style="padding-left:25px" >
